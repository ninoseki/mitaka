{
  "manifest_version": 2,
  "name": "Mitaka",
  "description": "Chrome Extension sends a search query to urlscan.io via the context menu",
  "version": "0.3.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "persistent": false,
    "scripts": ["js/vendor.js", "js/background.js"]
  },
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "https://urlscan.io/api/v1/*"
  ]
}
