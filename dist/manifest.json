{
  "manifest_version": 2,
  "name": "Mitaka",
  "description": "OSINT friendly IOC (Indicator of Compromise) saerch tool",
  "version": "0.11.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "persistent": true,
    "scripts": ["js/vendor.js", "js/background.js"]
  },
  "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["js/content.js"]
   }
 ],
  "permissions": [
    "contextMenus",
    "notifications",
    "storage",
    "https://urlscan.io/api/v1/*",
    "https://www.virustotal.com/vtapi/v2/*"
  ]
}
