{
  "name": "mitaka",
  "displayName": "Mitaka",
  "version": "0.93.0",
  "description": "A browser extension for searching IP, domain, URL, hash, etc. via the context menu.",
  "scripts": {
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src --fix tests",
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "prepare": "husky install",
    "test": "jest tests/ --collect-coverage"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "author": "ninoseki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/mitaka.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@github/mini-throttle": "^2.1.0",
    "bulma": "^0.9.4",
    "ioc-extractor": "5.0.3",
    "js-base64": "^3.7.5",
    "js-sha256": "^0.9.0",
    "vue": "^3.2.47",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@jest/types": "^29.4.3",
    "@plasmohq/prettier-plugin-sort-imports": "^3.6.1",
    "@types/chrome": "^0.0.217",
    "@types/jest": "^29.4.0",
    "@types/js-base64": "3.3.1",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "coveralls": "3.1.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-webextension-mock": "^3.8.8",
    "lint-staged": "13.1.2",
    "plasmo": "^0.65.2",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.5"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "contextMenus",
      "storage",
      "notifications"
    ]
  }
}